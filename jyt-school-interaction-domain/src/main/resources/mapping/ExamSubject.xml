<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" > 
<mapper namespace="com.yjtoon.school.mapper.IExamSubjectMapper" >
  <resultMap id="BaseResultMap" type="com.yjtoon.school.domain.ExamSubject" >
          <id column="es_id" property="esId" jdbcType="BIGINT" />
            <result column="ex_id" property="exId" jdbcType="BIGINT" />
            <result column="subject_name" property="subjectName" jdbcType="VARCHAR" />
            <result column="subject_sort" property="subjectSort" jdbcType="INTEGER" />
            <result column="average_store" property="averageStore" jdbcType="DECIMAL" />
            <result column="total_score" property="totalScore" jdbcType="DECIMAL" />
            <result column="es_is_delete" property="esIsDelete" jdbcType="CHAR" />
            <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
            <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
  </resultMap>
   
  <sql id="baseColumns">
         es_id
         ,ex_id
         ,subject_name
         ,subject_sort
         ,average_store
         ,total_score
         ,es_is_delete
         ,create_time
         ,update_time
  </sql>
  
  <sql id="whereSql">
        <if test="esId != null">
         and es_id=#{esId}
		</if> 
        <if test="exId != null">
         and ex_id=#{exId}
		</if> 
        <if test="subjectName != null and subjectName!='' ">
         and subject_name=#{subjectName}
		</if>
        <if test="subjectSort != null">
         and subject_sort=#{subjectSort}
		</if> 
        <if test="averageStore != null and averageStore!='' ">
         and average_store=#{averageStore}
		</if>
        <if test="totalScore != null and totalScore!='' ">
         and total_score=#{totalScore}
		</if>
        <if test="esIsDelete != null and esIsDelete!='' ">
         and es_is_delete=#{esIsDelete}
		</if>
        <if test="createTime != null">
         and create_time=#{createTime}
		</if> 
        <if test="updateTime != null">
         and update_time=#{updateTime}
		</if> 
  </sql>
  
  	<select id="getById" resultMap="BaseResultMap">
		select <include refid="baseColumns"/>
		from exam_subject  where es_id =#{esId}
	</select>
	
	<select id="getByIds" resultMap="BaseResultMap">
	     select <include refid="baseColumns"/> from exam_subject  where es_id in
	     <foreach collection="array" item="item" index="index"  open="(" close=")" separator=",">
               #{item}
         </foreach>  
	</select>
	
	<select id="get" resultMap="BaseResultMap" parameterType="com.yjtoon.school.domain.ExamSubject">
		select <include refid="baseColumns"/>
		from exam_subject  where 1=1 <include refid="whereSql"/> limit 1
	</select>
	
	<select id="findAllList" resultMap="BaseResultMap">
	     select <include refid="baseColumns"/> from exam_subject
	</select>
	
	<select id="findList"  parameterType="com.yjtoon.school.domain.ExamSubject" resultMap="BaseResultMap">
	     select <include refid="baseColumns"/> from exam_subject where 1=1 <include refid="whereSql"/>
	      <if test="orderBy != null and orderBy!='' ">
                order by ${orderBy}  
            </if>
	</select>

	
	 <select id="getCount"  parameterType="com.yjtoon.school.domain.ExamSubject" resultType="java.lang.Integer">
         select count(1) from exam_subject where 1=1 <include refid="whereSql"/>
      </select>

    <select id="findEntityWrapper"  parameterType="com.yjtoon.framework.sqlplus.EntityWrapper" resultMap="BaseResultMap">
	     select <include refid="baseColumns"/> from exam_subject where 1=1  ${ew.getSqlSegment()} 
	</select>
	
      <insert id="insert" useGeneratedKeys="true" keyProperty="esId" parameterType="com.yjtoon.school.domain.ExamSubject">
