    </insert>
    
    <update id="updateById" parameterType="com.yjtoon.school.domain.ExamSubject">
	    update exam_subject  set
	     <trim suffixOverrides="," >
		        <if test="exId != null">
		        ex_id=#{exId},
				</if> 
		        <if test="subjectName != null and subjectName!='' ">
		       subject_name=#{subjectName},
				</if>
		        <if test="subjectSort != null">
		        subject_sort=#{subjectSort},
				</if> 
		        <if test="averageStore != null and averageStore!='' ">
		       average_store=#{averageStore},
				</if>
		        <if test="totalScore != null and totalScore!='' ">
		       total_score=#{totalScore},
				</if>
		        <if test="esIsDelete != null and esIsDelete!='' ">
		       es_is_delete=#{esIsDelete},
				</if>
		        <if test="createTime != null">
		        create_time=#{createTime},
				</if> 
		        <if test="updateTime != null">
		        update_time=#{updateTime},
				</if> 
		  </trim>
		 where es_id =#{esId}
	</update>

	 <update id="updateByIds">
          update exam_subject  set
	     <trim suffixOverrides="," >
		        <if test="entity.exId != null">
		        ex_id= #{entity.exId},
				</if> 
		        <if test="entity.subjectName != null and entity.subjectName != '' ">
		        subject_name= #{entity.subjectName},
				</if>
		        <if test="entity.subjectSort != null">
		        subject_sort= #{entity.subjectSort},
				</if> 
		        <if test="entity.averageStore != null and entity.averageStore != '' ">
		        average_store= #{entity.averageStore},
				</if>
		        <if test="entity.totalScore != null and entity.totalScore != '' ">
		        total_score= #{entity.totalScore},
				</if>
		        <if test="entity.esIsDelete != null and entity.esIsDelete != '' ">
		        es_is_delete= #{entity.esIsDelete},
				</if>
		        <if test="entity.createTime != null">
		        create_time= #{entity.createTime},
				</if> 
		        <if test="entity.updateTime != null">
		        update_time= #{entity.updateTime},
				</if> 
		  </trim>
		 where es_id in
		  <foreach collection="ids" item="item" index="index"  open="(" close=")" separator=",">
               #{item}
         </foreach>
	</update>
     
	
	<delete id="deleteById">
        delete from exam_subject where es_id =#{esId}
    </delete>
    
