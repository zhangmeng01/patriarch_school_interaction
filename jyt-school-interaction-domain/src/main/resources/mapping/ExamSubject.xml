        </foreach>  
	</delete>
    
    <delete id="delete" parameterType="com.yjtoon.school.domain.ExamSubject">
        delete from exam_subject where 1=1 <include refid="whereSql"/> 
    </delete>


	<select id="findByIds" resultMap="BaseResultMap">
		select <include refid="baseColumns"/> from exam_subject  where es_id in
		<foreach collection="array" item="item" index="index"  open="(" close=")" separator=",">
			#{item}
		</foreach>
	</select>

	<!--  根据EXID批量修改(删除这次考试对应的学科)-->
	<update id="updateByExId" parameterType="com.yjtoon.school.domain.ExamSubject">
		update exam_subject  set
		<trim suffixOverrides="," >
			<if test="subjectName != null and subjectName!='' ">
				subject_name=#{subjectName},
			</if>
			<if test="subjectSort != null">
				subject_sort=#{subjectSort},
			</if>
			<if test="averageStore != null and averageStore!='' ">
				average_store=#{averageStore},
			</if>
			<if test="totalScore != null and totalScore!='' ">
				total_score=#{totalScore},
			</if>
			<if test="esIsDelete != null and esIsDelete!='' ">
				es_is_delete=#{esIsDelete},
			</if>
			<if test="createTime != null">
				create_time=#{createTime},
			</if>
			<if test="updateTime != null">
				update_time=#{updateTime},
			</if>
		</trim>
		where ex_id=#{exId}
	</update>
	<select id="findTotalScore"  resultType="java.util.HashMap">
		SELECT sum(total_score) as total_score,ex_id as ex_id	FROM  exam_subject where 1=1 and ex_id in
		<foreach collection="exIdList" item="item" index="index"  open="(" close=")" separator=",">
			#{item}
		</foreach>
		group by ex_id order by create_time desc limit 0,6
	</select>
</mapper>